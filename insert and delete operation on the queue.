#include <stdio.h>
#define MAX 5

int queue[MAX];
int front = -1, rear = -1;

/* Insert operation */
void insert() {
    int x;
    if (rear == MAX - 1) {
        printf("Queue Overflow\n");
    } else {
        if (front == -1)
            front = 0;
        printf("Enter element to insert: ");
        scanf("%d", &x);
        queue[++rear] = x;
        printf("Element inserted successfully\n");
    }
}

/* Delete operation */
void delete() {
    if (front == -1 || front > rear) {
        printf("Queue Underflow\n");
    } else {
        printf("Deleted element = %d\n", queue[front++]);
    }
}

int main() {
    int choice;

    while (1) {
        printf("\n1. Insert\n2. Delete\n3. Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1: insert();
                    break;
            case 2: delete();
                    break;
            case 3: return 0;
            default: printf("Invalid choice\n");
        }
    }
}
