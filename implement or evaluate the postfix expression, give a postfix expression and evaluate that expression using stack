#include <stdio.h>
#include <ctype.h>

int stack[50];
int top = -1;

/* Push operation */
void push(int x) {
    stack[++top] = x;
}

/* Pop operation */
int pop() {
    return stack[top--];
}

int main() {
    char postfix[50];
    int i, op1, op2, result;

    printf("Enter postfix expression: ");
    scanf("%s", postfix);

    for (i = 0; postfix[i] != '\0'; i++) {

        if (isdigit(postfix[i])) {
            push(postfix[i] - '0');   // convert char to int
        } 
        else {
            op2 = pop();
            op1 = pop();

            switch (postfix[i]) {
                case '+': result = op1 + op2; break;
                case '-': result = op1 - op2; break;
                case '*': result = op1 * op2; break;
                case '/': result = op1 / op2; break;
            }
            push(result);
        }
    }

    printf("Result = %d", pop());
    return 0;
}
