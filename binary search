#include <stdio.h>

int main() {
    int a[5] = {10, 20, 30, 40, 50};
    int low = 0, high = 4, mid;
    int key = 40;

    while (low <= high) {
        mid = (low + high) / 2;

        if (a[mid] == key) {
            printf("Element found at index %d", mid);
            return 0;
        }
        else if (a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }

    printf("Element not found");
    return 0;
}
